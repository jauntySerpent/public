{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
	  	{
          "type": "root",
		  "foreground": "red",
		  "style": "plain",
          "template": "[ADMIN] "
        },
        {
          "foreground": "lightYellow",
          "style": "plain",
          "template": "{{ .UserName }} ",
          "type": "session"
        },
        {
          "foreground": "cyan",
          "properties": {
            "style": "folder"
          },
          "style": "plain",
          "template": "<#ffffff>in</> {{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "#ff94df",
          "properties": {
            "branch_icon": " <#ff94df><b>\ue0a0 </b></>",
            "fetch_stash_count": true
          },
          "style": "plain",
          "template": "<#ffffff>on</> {{ .HEAD }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
	{
	  "alignment": "right",
	  "segments": [
	    {
		  "type": "executiontime",
		  "style": "plain",
		  "foreground": "#ffffff",
		  "template": "\uf253 {{ .FormattedMs }}",
		  "properties": {
		    "style": "amarillo",
			"threshold": 0
		  }
		}
	  ],
	  "type": "prompt"
	},
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "lightGreen",
          "style": "plain",
          "template": "\u276f",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "valid_line": {
    "foreground": "Green",
	"template": "\u276f"
  },
  "error_line": {
	"foreground": "Red",
	"template": "\u276f"
  },
  "final_space": true,
  "version": 2
}
